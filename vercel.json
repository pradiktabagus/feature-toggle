{
  "functions": {
    "src/app/api/public/toggles/[key]/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=300"
        },
        {
          "key": "CDN-Cache-Control", 
          "value": "max-age=300"
        }
      ]
    }
  ]
}